set mbox_type="Maildir"
set folder="~/Mail"
set mailcap_path="~/.mutt/mailcap"

auto_view text/html
alternative_order text/plain text/html

set spoolfile="+AAU/INBOX"
folder-hook '+AAU/*' 'source ~/.mutt/accounts/AAU'
folder-hook '+AU/*' 'source ~/.mutt/accounts/AU'

auto_view text/html
alternative_order text/plain text/html

set ssl_starttls=yes

set pgp_use_gpg_agent=yes
set pgp_timeout=600
